{
  "batch_norm": true,
  "batch_size": 256,
  "scale_inputs": true,
  "scale_outputs": true,
  "normalize": {
    "with_mean": true,
    "with_std": false
  },
  "loss": "l2",
  "activation_function": "relu",
  "hidden_layers": [
    {
      "type": "fc",
      "units": 512
    },
    {
      "type": "fc",
      "units": 512
    },
    {
      "type": "fc",
      "units": 512
    },
    {
      "type": "fc",
      "units": 512
    },
    {
      "type": "fc",
      "units": 512
    }
  ],
  "no_dwis": 288,
  "no_epochs": 200,
  "early_stopping": 15,
  "optimizer": {
    "beta1": 0.9,
    "beta2": 0.999,
    "epsilon": 1e-08,
    "learning_rate": 1e-04,
    "type": "adam"
  }
}
